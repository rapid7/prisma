{
  "author": "Tony Quetano <tony_quetano@rapid7.com>",
  "bugs": {
    "url": "https://github.com/rapid7/prisma/issues"
  },
  "description": "String-to-color generator",
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.10.1",
    "babel-eslint": "6.1.0",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "eslint": "3.0.1",
    "eslint-friendly-formatter": "2.0.5",
    "eslint-loader": "1.4.0",
    "eslint-plugin-import": "1.10.2",
    "eslint-plugin-react": "5.2.2",
    "html-webpack-plugin": "2.22.0",
    "in-publish": "^2.0.0",
    "inline-style-prefixer": "2.0.0",
    "json-loader": "0.5.4",
    "lodash": "4.13.1",
    "react": "15.2.0",
    "react-dom": "15.2.0",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "homepage": "https://github.com/rapid7/prisma#readme",
  "keywords": [
    "color",
    "string",
    "rgb",
    "rgba",
    "hsl",
    "hsla",
    "hexadecimal",
    "hex"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "name": "prisma",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapid7/prisma.git"
  },
  "scripts": {
    "build": "NODE_ENV=development ./node_modules/webpack/bin/webpack.js --progress --config=webpack.config.js",
    "build-minified": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --progress --config=webpack.config.minified.js",
    "compile-for-publish": "npm run lint && npm run transpile && npm run dist",
    "dev": "NODE_ENV=development ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --config=webpack.config.dev.js",
    "dist": "npm run build && npm run build-minified",
    "lint": "NODE_ENV=test eslint src",
    "prepublish": "in-publish && npm run compile-for-publish || in-install",
    "transpile": "./node_modules/babel-cli/bin/babel.js -d lib/ src/"
  },
  "version": "3.1.0"
}
